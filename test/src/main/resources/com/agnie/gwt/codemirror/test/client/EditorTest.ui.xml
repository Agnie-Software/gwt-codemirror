<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:cm="urn:import:com.agnie.gwt.codemirror.client">

	<g:HTMLPanel>
		<cm:CodeMirror mode="HTML_MIXED" theme="MONOKAI"
			lineNumbers="true" autoCloseBrackets="true" keyMap="SUB_LIME"
			autoCloseTags="true" matchBrackets="true" matchTags="true" ui:field="editor"></cm:CodeMirror>
	</g:HTMLPanel>
</ui:UiBinder> 